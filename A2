#include <conio.h>
#include <stdio.h>
#define pi 3.14
int main ()
{
  float radio;
  float altura;
  float resultado;
  printf("cual es el radio:");
  scanf("%f",&radio);
  printf("cual es la altura");
  scanf("%f",&altura);
  resultado=1/((3*Pi)*(radio*radio)*altura);
  printf("el volumen del cono es:%f",resultado);
  getch ();
  return 0;
}
